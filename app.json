{
  "expo": {
    "name": "reataurant-advisor",
    "slug": "reataurant-advisor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00a680"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "package": "com.axvargas.reataurantadvisor",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC-PN5FRGnHJ9DqTXxcjs7RefA0rVPoiGE"
        }
      },
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "mobile app built with react-native",
    "githubUrl": "https://github.com/axvargas/restaurant-advisor.git"
  }
}
